{
  "name": "bible-api",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "jest --watchAll",
    "build": "tsc --build",
    "build:docs": "cd docs && pnpm build",
    "dev:docs": "vuepress dev docs/src",
    "cli": "ts-node ./packages/helloao-cli/cli.ts",
    "s3:sync:docs": "aws --profile aolabs s3 sync ./build/docs s3://ao-bible-api/docs --delete",
    "s3:sync:api": "npm run cli upload-api-files s3://ao-bible-api -- --profile aolabs --overwrite-common-files",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.4.0",
    "@types/lodash": "4.14.191",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.17",
    "jest": "29.5.0",
    "jest-raw-loader": "1.0.1",
    "ts-jest": "29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3"
  },
  "dependencies": {},
  "prisma": {
    "schema": "./packages/helloao-tools/schema.prisma"
  },
  "engines": {
    "pnpm": "^8.11.0"
  }
}
