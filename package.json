{
  "name": "bible-api",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test": "jest --watchAll",
    "build": "ts-node ./src/generate-api.ts",
    "build:docs": "cd docs && yarn install && yarn build",
    "dev:docs": "vuepress dev docs/src",
    "preview": "vite preview",
    "generate": "ts-node src/generate-api.ts",
    "build-api": "yarn install && yarn build && yarn build:docs && yarn build:read-site",
    "dev:g": "gatsby develop",
    "build:read-site": "gatsby build && cp -nr ./public/* ./build/"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.4.0",
    "@types/lodash": "4.14.191",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-vue": "^4.0.0",
    "gatsby": "4.24.0",
    "gatsby-source-filesystem": "4.25.0",
    "hash.js": "1.1.7",
    "jest": "29.5.0",
    "jest-raw-loader": "1.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-jest": "29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "better-sqlite3": "^9.5.0",
    "fs-extra": "^11.2.0",
    "highlight.js": "^11.7.0",
    "lodash": "4.17.21",
    "markdown-it": "^13.0.1",
    "react-select": "5.7.0",
    "vue": "^3.2.45",
    "vue-router": "4"
  },
  "prisma": {
    "schema": "./src/schema.prisma"
  }
}
